    <style name="AppBarBottomSheetDialogDark" parent="Theme.Design.BottomSheetDialog">
        <item name="enforceTextAppearance">true</item>
        <item name="enforceMaterialTheme">false</item>
        <item name="itemIconSize">@dimen/app_bottom_navigation_icon_size</item>
        <item name="bottomSheetStyle">@style/AppBarBottomSheetStyleDark</item>
    </style>
</resources>