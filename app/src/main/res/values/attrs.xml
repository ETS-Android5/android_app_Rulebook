<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppRatingVector">
        <attr name="face_color" format="color"/>
        <attr name="eyes_color" format="color"/>
        <attr name="tongue_color" format="color"/>
        <attr name="mouth_color" format="color"/>
        <attr name="default_rating" format="integer"/>
    </declare-styleable>

    <attr format="reference|color" name="backgroundColor"/>
    <attr format="reference|color" name="backgroundColorLight"/>
    <attr format="reference|color" name="backgroundColorDark"/>
    <attr format="reference|color" name="menuIconColor"/>
    <attr format="reference|color" name="subMenuIconColor"/>

    <declare-styleable name="FontDecorator">
        <attr format="string" name="customThemeEngineFont"/>
    </declare-styleable>

    <declare-styleable name="ColorPanelView">
        <attr format="boolean" name="cpv_showOldColor"/>
        <attr format="enum" name="cpv_colorShape">
            <enum name="square" value="0"/>
            <enum name="circle" value="1"/>
        </attr>
        <attr format="color|reference" name="cpv_borderColor"/>
    </declare-styleable>

    <declare-styleable name="ColorPickerView" parent="ColorPanelView">
        <attr format="boolean|reference" name="cpv_alphaChannelVisible"/>
        <attr format="string|reference" name="cpv_alphaChannelText"/>
        <attr format="color|reference" name="cpv_sliderColor"/>
        <attr name="cpv_borderColor"/>
    </declare-styleable>

    <declare-styleable name="ColorPreference" parent="ColorPickerDialog">
        <attr format="boolean|reference" name="cpv_showAlphaSlider"/>
        <attr format="enum" name="cpv_previewSize">
            <enum name="regular" value="0"/>
            <enum name="large" value="1"/>
        </attr>
        <attr name="cpv_colorShape"/>
        <attr format="reference" name="cpv_dialogTitle"/>
        <attr format="reference" name="cpv_colorPresets"/>
        <attr format="enum" name="cpv_dialogType">
            <enum name="custom" value="0"/>
            <enum name="preset" value="1"/>
        </attr>
        <attr format="boolean|reference" name="cpv_showColorShades"/>
        <attr format="boolean|reference" name="cpv_allowPresets"/>
        <attr format="boolean|reference" name="cpv_allowCustom"/>
        <attr format="boolean|reference" name="cpv_showDialog"/>
    </declare-styleable>

    <attr name="shapeType" format="enum">
        <enum name="circle" value="1" />
        <enum name="square" value="2" />
    </attr>
    <attr name="progress" format="integer" />
    <attr name="max" format="integer" />
    <attr name="borderWidthSize" format="dimension" />
    <attr name="frontColor" format="color" />
    <attr name="behideColor" format="color" />
    <attr name="borderColor" format="color" />
    <attr name="strong" format="integer" />
    <attr name="textColor" format="color" />
    <attr name="animatorEnable" format="boolean" />
    <attr name="textHidden" format="boolean" />
    <attr name="shapePadding" format="dimension" />

    <declare-styleable name="WaveView">
        <attr name="progress" />
        <attr name="max" />
        <attr name="strong" />
        <attr name="frontColor" />
        <attr name="behideColor" />
        <attr name="borderColor" />
        <attr name="borderWidthSize" />
        <attr name="textColor" />
        <attr name="animatorEnable" />
        <attr name="shapeType" />
        <attr name="textHidden" />
        <attr name="shapePadding" />
    </declare-styleable>

    <declare-styleable name="StyleableToast">
        <attr name="stTextBold" format="boolean" />
        <attr name="stTextSize" format="dimension" />
        <attr name="stColorBackground" format="color" />
        <attr name="stRadius" format="dimension" />
        <attr name="stStrokeWidth" format="dimension" />
        <attr name="stIconStart" format="reference" />
        <attr name="stIconEnd" format="reference" />
        <attr name="stLength" format="integer">
            <enum name="LONG" value="1" />
            <enum name="SHORT" value="0" />
        </attr>
        <attr name="stGravity" format="integer">
            <enum name="center" value="1" />
            <enum name="top" value="2" />
        </attr>

    </declare-styleable>
</resources>